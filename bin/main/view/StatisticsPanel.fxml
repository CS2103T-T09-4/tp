<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="placeHolder" styleClass="pane-with-border"
  xmlns="http://javafx.com/javafx/11.0.3-internal" xmlns:fx="http://javafx.com/fxml/1">
  <VBox alignment="CENTER">
    <LineChart fx:id="statisticsLineChart" title="Stat Chart" BorderPane.alignment="CENTER">
      <xAxis>
        <CategoryAxis side="BOTTOM"/>
      </xAxis>
      <yAxis>
        <NumberAxis side="LEFT"/>
      </yAxis>
    </LineChart>
    <VBox alignment="CENTER">
      <Label fx:id="lastLoginLabel" HBox.hgrow="ALWAYS" styleClass="header-message"/>
    </VBox>
  </VBox>
</StackPane>
